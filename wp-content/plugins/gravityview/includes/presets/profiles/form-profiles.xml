<?xml version="1.0" encoding="UTF-8"?>
<forms version="1.8.7">
	<form labelPlacement="top_label" useCurrentUserAsAuthor="1">
			<title><![CDATA[GravityView - Profiles]]></title>
			<descriptionPlacement><![CDATA[below]]></descriptionPlacement>
			<button type="text">
				<text><![CDATA[Submit]]></text>
			</button>
			<fields>
				<field id="1" size="medium" type="name" nameFormat="normal">
					<label><![CDATA[Name]]></label>
					<inputs>
						<input id="1.3">
							<label><![CDATA[First]]></label>
						</input>
						<input id="1.6">
							<label><![CDATA[Last]]></label>
						</input>
					</inputs>
				</field>
				<field id="7" size="medium" type="email">
					<label><![CDATA[Email]]></label>
				</field>
				<field id="8" size="medium" type="phone" phoneFormat="standard">
					<label><![CDATA[Phone]]></label>
				</field>
				<field id="10" size="medium" type="text">
					<label><![CDATA[Work]]></label>
				</field>
				<field id="11" size="medium" type="address" addressType="us">
					<label><![CDATA[Address]]></label>
					<defaultCountry><![CDATA[United States]]></defaultCountry>
					<inputs>
						<input id="11.1">
							<label><![CDATA[Street Address]]></label>
						</input>
						<input id="11.2">
							<label><![CDATA[Address Line 2]]></label>
						</input>
						<input id="11.3">
							<label><![CDATA[City]]></label>
						</input>
						<input id="11.4">
							<label><![CDATA[State / Province]]></label>
						</input>
						<input id="11.5">
							<label><![CDATA[ZIP / Postal Code]]></label>
						</input>
						<input id="11.6">
							<label><![CDATA[Country]]></label>
						</input>
					</inputs>
				</field>
				<field id="4" size="medium" type="textarea">
					<label><![CDATA[Bio]]></label>
				</field>
				<field id="9" size="medium" type="fileupload">
					<label><![CDATA[Photo]]></label>
					<allowedExtensions><![CDATA[jpg,jpeg,gif,png]]></allowedExtensions>
				</field>
				<field id="5" size="medium" type="list">
					<label><![CDATA[Interests]]></label>
				</field>
				<field id="2" size="medium" type="text">
					<cssClass><![CDATA[validate-facebook]]></cssClass>
					<errorMessage><![CDATA[Please enter a valid Facebook account name or page.]]></errorMessage>
					<label><![CDATA[Facebook Profile]]></label>
				</field>
				<field id="3" size="medium" type="text">
					<cssClass><![CDATA[validate-twitter]]></cssClass>
					<errorMessage><![CDATA[Please enter a valid Twitter account username.]]></errorMessage>
					<label><![CDATA[Twitter Username]]></label>
				</field>
				<field id="6" size="medium" type="website">
					<label><![CDATA[Website]]></label>
				</field>
			</fields>
			<notifications>
				<notification id="534df853d3cc5">
					<to><![CDATA[{admin_email}]]></to>
					<name><![CDATA[Admin Notification]]></name>
					<event><![CDATA[form_submission]]></event>
					<toType><![CDATA[email]]></toType>
					<subject><![CDATA[New submission from {form_title}]]></subject>
					<message><![CDATA[{all_fields}]]></message>
				</notification>
			</notifications>
			<confirmations>
				<confirmation id="534df853d40da" isDefault="1" type="message">
					<name><![CDATA[Default Confirmation]]></name>
					<message><![CDATA[Thanks for contacting us! We will get in touch with you shortly.]]></message>
				</confirmation>
			</confirmations>
		</form>
</forms>
