<?xml version="1.0" encoding="UTF-8"?>
<forms version="1.8.7">
	<form labelPlacement="top_label" useCurrentUserAsAuthor="1">
			<title><![CDATA[GravityView - Resume Board]]></title>
			<descriptionPlacement><![CDATA[below]]></descriptionPlacement>
			<button type="text">
				<text><![CDATA[Submit]]></text>
			</button>
			<fields>
				<field id="20" size="medium" type="section" displayOnly="1">
					<label><![CDATA[Contact Information]]></label>
				</field>
				<field id="21" size="medium" type="name">
					<label><![CDATA[Name]]></label>
					<inputs>
						<input id="21.3">
							<label><![CDATA[First]]></label>
						</input>
						<input id="21.6">
							<label><![CDATA[Last]]></label>
						</input>
					</inputs>
				</field>
				<field id="24" size="medium" type="email">
					<label><![CDATA[Email]]></label>
				</field>
				<field id="26" size="medium" type="phone" phoneFormat="standard">
					<label><![CDATA[Phone]]></label>
				</field>
				<field id="16" size="medium" type="address" addressType="us" hideAddress2="1">
					<label><![CDATA[Location]]></label>
					<defaultCountry><![CDATA[United States]]></defaultCountry>
					<inputs>
						<input id="16.1">
							<label><![CDATA[Street Address]]></label>
						</input>
						<input id="16.2">
							<label><![CDATA[Address Line 2]]></label>
						</input>
						<input id="16.3">
							<label><![CDATA[City]]></label>
						</input>
						<input id="16.4">
							<label><![CDATA[State / Province]]></label>
						</input>
						<input id="16.5">
							<label><![CDATA[ZIP / Postal Code]]></label>
						</input>
						<input id="16.6">
							<label><![CDATA[Country]]></label>
						</input>
					</inputs>
					<salesforceMapType><![CDATA[live]]></salesforceMapType>
				</field>
				<field id="27" size="medium" type="section" displayOnly="1">
					<label><![CDATA[Desired Employment]]></label>
					<salesforceMapType><![CDATA[live]]></salesforceMapType>
				</field>
				<field id="29" size="medium" type="text">
					<label><![CDATA[Job Title]]></label>
					<salesforceMapType><![CDATA[live]]></salesforceMapType>
				</field>
				<field id="17" size="medium" type="multiselect" enableEnhancedUI="1">
					<label><![CDATA[Desired Employment Type]]></label>
					<choices>
						<choice>
							<text><![CDATA[Full Time]]></text>
							<value><![CDATA[Full Time]]></value>
						</choice>
						<choice>
							<text><![CDATA[Part Time]]></text>
							<value><![CDATA[Part Time]]></value>
						</choice>
						<choice>
							<text><![CDATA[Contractor]]></text>
							<value><![CDATA[Contractor]]></value>
						</choice>
						<choice>
							<text><![CDATA[Intern]]></text>
							<value><![CDATA[Intern]]></value>
						</choice>
						<choice>
							<text><![CDATA[Seasonal]]></text>
							<value><![CDATA[Seasonal]]></value>
						</choice>
					</choices>
					<salesforceMapType><![CDATA[live]]></salesforceMapType>
				</field>
				<field id="18" size="medium" type="product" inputType="price">
					<label><![CDATA[Desired Salary]]></label>
					<basePrice><![CDATA[$0.00]]></basePrice>
					<salesforceMapType><![CDATA[live]]></salesforceMapType>
				</field>
				<field id="19" size="medium" type="select">
					<label><![CDATA[Salary Type]]></label>
					<choices>
						<choice>
							<text><![CDATA[Contract]]></text>
						</choice>
						<choice>
							<text><![CDATA[Per Hour]]></text>
						</choice>
						<choice>
							<text><![CDATA[Per Week]]></text>
						</choice>
						<choice>
							<text><![CDATA[Twice Monthly]]></text>
						</choice>
						<choice isSelected="1">
							<text><![CDATA[Per Month]]></text>
						</choice>
						<choice>
							<text><![CDATA[Per Year]]></text>
						</choice>
					</choices>
					<salesforceMapType><![CDATA[live]]></salesforceMapType>
				</field>
				<field id="28" size="medium" type="section" displayOnly="1">
					<label><![CDATA[Experience & Qualifications]]></label>
					<salesforceMapType><![CDATA[live]]></salesforceMapType>
				</field>
				<field id="5" size="medium" type="textarea">
					<label><![CDATA[Objective]]></label>
				</field>
				<field id="3" size="large" type="textarea">
					<label><![CDATA[Skills]]></label>
				</field>
				<field id="22" size="large" type="textarea">
					<label><![CDATA[Education]]></label>
				</field>
				<field id="25" size="medium" type="list">
					<label><![CDATA[Languages]]></label>
				</field>
				<field id="23" size="medium" type="fileupload">
					<label><![CDATA[Resume & CV]]></label>
					<allowedExtensions><![CDATA[doc,pdf,docx,pdfx,txt,rtf,pages]]></allowedExtensions>
					<multipleFiles><![CDATA[1]]></multipleFiles>
				</field>
			</fields>
			<notifications>
				<notification id="534cb7a024c95">
					<to><![CDATA[{admin_email}]]></to>
					<name><![CDATA[Admin Notification]]></name>
					<event><![CDATA[form_submission]]></event>
					<toType><![CDATA[email]]></toType>
					<subject><![CDATA[New submission from {form_title}]]></subject>
					<message><![CDATA[{all_fields}]]></message>
				</notification>
			</notifications>
			<confirmations>
				<confirmation id="534cb7a024fa9" isDefault="1" type="message">
					<name><![CDATA[Default Confirmation]]></name>
					<message><![CDATA[Thanks for contacting us! We will get in touch with you shortly.]]></message>
				</confirmation>
			</confirmations>
		</form>
</forms>
